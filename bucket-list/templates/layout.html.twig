<!doctype html>
<html lang="fr">
<head> {% include'/inc/head.html.twig' %}

</head>
<body>
{% include '/inc/header.html.twig' %}
{% for label, messages in app.flashes %}
{% for message in messages %}
<div class="alert-{{ label }}">
    {{ message }}
</div>
{% endfor %}
{% endfor %}
<div class="container">
    {% block title %} World's Famous Bucket List{% endblock %}
    <main>
        {% block main %}{% endblock %}
    </main>
</div>
<footer>
    {% include'/inc/footer.html.twig' %}
</footer>
</body>
</html>